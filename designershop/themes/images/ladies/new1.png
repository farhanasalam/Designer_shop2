                   <script>     
$("document").ready(function(){
  $.ajaxSetup({
        error: function(jqXHR, exception) {
            if (jqXHR.status === 0) {
                 $('#response').html('<div class="alert alert-danger" role="alert">Network Busy !!</div>');
                  $("#result").html('');
            }else {
                 $('#response').html('<div class="alert alert-danger" role="alert">Server is busy.Please try after some time!</div>');
                  $("#result").html('');
            }
        }
    });
  $("#orderstat").submit(function(){
    ga(['send', 'Order Track', 'Track order', 'Order Tracking']);
    if($("#id").val()==''){
        $('#response').html('<div class="alert alert-danger" role="alert">Enter Customer Code!!</div>');
    
    }else{

    var data = {
      "action": "order_status"
    };
    data = $(this).serialize() + "&" + $.param(data);
    $("#result").html('<img src="http://parisdeboutique.in/images/loading.gif" />');
    $.ajax({
      type: "POST",
      dataType: "json",
      url: "http://122.165.154.53/parismis/order_status.php", //Relative or absolute path to response.php file
      data: data,
      success: function(data) {
       // console.log(data);
       var respdata = '';
         var json = $.parseJSON(data.json);
                  if(json.length>0){
         $.each(json,function(i,value){
            console.log(value);

            $.each(value,function(key,value){
              var j= i+1;
                respdata += j+'.'+key+' - '+value;
                if(value!='Processing')
                    respdata +=' <i class="glyphicon glyphicon-ok"></i>';
                    
                respdata +='<hr>';
            });
          
         });
         }else
              respdata = 'Sorry,No Orders Found';
         $('#result').html(respdata);
        

      }
    });
    
}
return false;
  });
